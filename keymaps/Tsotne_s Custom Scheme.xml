<keymap version="1" name="Tsotne's Custom Scheme" parent="$default">
  <action id="$Copy">
    <keyboard-shortcut first-keystroke="ctrl c" />
  </action>
  <action id="$Cut">
    <keyboard-shortcut first-keystroke="ctrl x" />
  </action>
  <action id="$Paste">
    <keyboard-shortcut first-keystroke="ctrl v" />
  </action>
  <action id="Back">
    <mouse-shortcut keystroke="button4" />
    <keyboard-shortcut first-keystroke="altGraph left" />
  </action>
  <action id="CommentByBlockComment">
    <keyboard-shortcut first-keystroke="shift ctrl divide" />
    <keyboard-shortcut first-keystroke="shift ctrl colon" />
  </action>
  <action id="CommentByLineComment">
    <keyboard-shortcut first-keystroke="ctrl divide" />
    <keyboard-shortcut first-keystroke="ctrl colon" />
  </action>
  <action id="DecreaseColumnWidth" />
  <action id="EditorNextWord" />
  <action id="EditorNextWordInDifferentHumpsMode">
    <keyboard-shortcut first-keystroke="ctrl right" />
  </action>
  <action id="EditorNextWordInDifferentHumpsModeWithSelection">
    <keyboard-shortcut first-keystroke="shift ctrl right" />
  </action>
  <action id="EditorNextWordWithSelection" />
  <action id="EditorPreviousWord" />
  <action id="EditorPreviousWordInDifferentHumpsMode">
    <keyboard-shortcut first-keystroke="ctrl left" />
  </action>
  <action id="EditorPreviousWordInDifferentHumpsModeWithSelection">
    <keyboard-shortcut first-keystroke="shift ctrl left" />
  </action>
  <action id="EditorPreviousWordWithSelection" />
  <action id="Forward">
    <mouse-shortcut keystroke="button5" />
    <keyboard-shortcut first-keystroke="altGraph right" />
  </action>
  <action id="IncreaseColumnWidth" />
  <action id="IncrementWindowWidth" />
  <action id="NewFile">
    <keyboard-shortcut first-keystroke="shift insert" />
  </action>
  <action id="ResizeToolWindowLeft" />
  <action id="ResizeToolWindowRight" />
  <action id="RevealIn">
    <keyboard-shortcut first-keystroke="shift ctrl s" />
  </action>
</keymap>